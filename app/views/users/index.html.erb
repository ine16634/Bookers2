<%= render 'books/myinfo' %>
<%= render 'books/new' %>



<h1>Users</h1>

<div class="User-wrap">
  <table class="table-box">
    <thead>
      <tr>
        <th>Title</th>
        <th>Opinion</th>
        <th colspan="1"></th>

      </tr>
    </thead>

    <tbody>
    <% @users.each do |user| %>
      <tr>
         <td><%= image_tag user.get_profile_image(100,100) %></td>
         <td><%= user.name %></td>
         <td><%=link_to "show", user_path(user.id) %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
  </div>
  
  